<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardView"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginTop="16dp"
    android:layout_marginEnd="16dp"
    android:layout_marginStart="16dp"
    card_view:cardCornerRadius="4dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="118dp">

        <TextView
            android:id="@+id/nombre"
            android:layout_width="143dp"
            android:layout_height="35dp"
            android:layout_marginEnd="16dp"
            android:padding="8dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintEnd_toEndOf="parent"
            card_view:layout_constraintHorizontal_bias="0.478"
            card_view:layout_constraintStart_toEndOf="@+id/tipo"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.192"
            tools:text="Pikachu" />

        <TextView
            android:id="@+id/tipo"
            android:layout_width="181dp"
            android:layout_height="37dp"
            android:layout_marginStart="16dp"
            android:padding="8dp"
            card_view:layout_constraintBottom_toTopOf="@+id/supermercado"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.533"
            tools:text="Pikachu" />

        <TextView
            android:id="@+id/supermercado"
            android:layout_width="178dp"
            android:layout_height="36dp"
            android:layout_marginStart="16dp"
            android:padding="8dp"
            card_view:layout_constraintBottom_toBottomOf="parent"
            card_view:layout_constraintStart_toStartOf="parent"
            card_view:layout_constraintTop_toTopOf="parent"
            card_view:layout_constraintVertical_bias="0.807"
            tools:text="Pikachu" />


    </androidx.constraintlayout.widget.ConstraintLayout>


</androidx.cardview.widget.CardView>